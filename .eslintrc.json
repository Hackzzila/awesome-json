{
  "extends": "airbnb",
  "parserOptions": {
    "emcaVersion": 6
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "strict": 0,
    "global-require": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": true, "peerDependencies": true}],
    "no-restricted-syntax": 0,
    "guard-for-in": 0
  }
}